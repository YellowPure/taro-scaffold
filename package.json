{
  "name": "taro-scaffold",
  "version": "1.0.0",
  "private": true,
  "description": "a taro scaffold",
  "templateInfo": {
    "name": "mobx",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build-test:weapp": "taro build --type weapp --env test",
    "build-stage:weapp": "taro build --type weapp --env stage",
    "build-prod:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "taro build --type weapp --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "upload-cdn": "gulp mini && gulp upload-cdn",
    "compress": "gulp build",
    "commit-msg-check": "commitlint -E HUSKY_GIT_PARAMS",
    "prettier": "prettier --write ./src",
    "lint": "eslint --fix --ext .ts --ext .tsx src"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.2.8",
    "@tarojs/react": "3.2.8",
    "@tarojs/runtime": "3.2.8",
    "@tarojs/taro": "3.2.8",
    "agentkeepalive": "^4.1.3",
    "ali-oss": "^6.10.0",
    "async": "^3.2.0",
    "del": "^5.1.0",
    "immer": "^7.0.9",
    "mime-types": "^2.1.27",
    "mobx": "^4.8.0",
    "mobx-react": "^6.1.4",
    "mockjs": "^1.1.0",
    "number-precision": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "taro-ui": "3.0.0-alpha.3",
    "use-immer": "^0.4.1",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@tarojs/cli": "3.2.8",
    "@tarojs/mini-runner": "3.2.8",
    "@tarojs/webpack-runner": "3.2.8",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-preset-taro": "3.2.8",
    "eslint": "^7.29.0",
    "eslint-config-ali": "^12.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-taro": "3.2.8",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "gulp": "^4.0.2",
    "gulp-cssnano": "^2.1.3",
    "gulp-htmlclean": "^2.7.22",
    "gulp-imagemin": "^7.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-uglify": "^3.0.2",
    "lint-staged": "^11.0.0",
    "node-gyp": "^7.1.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix --ext .ts --ext .tsx src",
      "prettier --write"
    ],
    "**/*.less": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "npm run commit-msg-check"
    }
  }
}
